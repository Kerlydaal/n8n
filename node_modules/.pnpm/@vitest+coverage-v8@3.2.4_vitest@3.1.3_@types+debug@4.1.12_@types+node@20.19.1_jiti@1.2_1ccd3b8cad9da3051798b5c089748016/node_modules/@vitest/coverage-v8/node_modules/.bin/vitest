#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/n8n/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.1_jiti@1.21.7_jsdom@23.0.1_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vitest/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.1_jiti@1.21.7_jsdom@23.0.1_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/n8n/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.1_jiti@1.21.7_jsdom@23.0.1_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vitest/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.1_jiti@1.21.7_jsdom@23.0.1_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.1_jiti@1.21.7_jsdom@23.0.1_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.1_jiti@1.21.7_jsdom@23.0.1_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vitest/vitest.mjs" "$@"
fi
