#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/playwright/dist/bin/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/playwright/dist/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/playwright/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/playwright/dist/bin/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/playwright/dist/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/playwright/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules/@currents/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/@currents+playwright@1.14.1_magicast@0.3.5/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@currents/playwright/dist/bin/pwc-p.js" "$@"
else
  exec node  "$basedir/../@currents/playwright/dist/bin/pwc-p.js" "$@"
fi
