#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules/mqtt/build/bin/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules/mqtt/build/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules/mqtt/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules/mqtt/build/bin/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules/mqtt/build/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules/mqtt/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/mqtt@5.7.2/node_modules:/home/runner/work/n8n/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mqtt/build/bin/sub.js" "$@"
else
  exec node  "$basedir/../mqtt/build/bin/sub.js" "$@"
fi
